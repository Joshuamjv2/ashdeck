{
  "manifest_version": 3,
  "description": "Focus and Productive tool to help reduce distractions.",
  "name": "Website Blocker",
  "version": "0.1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "storage",
    "declarativeNetRequest"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/ashdeck-logo.jpg"
  }
}
